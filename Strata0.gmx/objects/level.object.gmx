            <string>// Globals
globalvar is_paused;
global.is_paused = false;

// Set up timers
_start_time = current_time;
_current_index = 0;

// Read level Data
_file = file_text_open_read("level.txt");
_num_steps = file_text_read_real(_file);
file_text_readln(_file);

_times = 0;
_actions = 0;

for(i = 0; i &lt; _num_steps; i += 1) {
    _times[i] = file_text_read_real(_file);
    _actions[i] = file_text_read_string(_file);
    _actions[i] = string_delete(_actions[i], 1, 1);
    file_text_readln(_file);
}

var snd = audio_play_sound(music, 10, false);
audio_sound_set_track_position(snd, 41);

