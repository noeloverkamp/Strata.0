            <string>var time_elapsed = current_time - _start_time;
while(_current_index &lt; _num_steps &amp;&amp; time_elapsed &gt; _times[_current_index]) {
    //show_debug_message(_actions[_current_index]);
    if(string_count("pause", _actions[_current_index]) == 1) {
        global.is_paused = true;
    }
    else if(string_count("start", _actions[_current_index]) == 1) {
        global.is_paused = false;
    }
    else if(string_count("spawn", _actions[_current_index]) == 1) {
        var xpos = string_copy(_actions[_current_index], 7, 3);
        var ypos = string_copy(_actions[_current_index], 11, 3);
        var obj = instance_create(real(xpos), real(ypos), obj_mine);
        var dir = string_copy(_actions[_current_index], 13, 3);
        obj._dir = dir;
    }
    _current_index += 1;
}
</string>
